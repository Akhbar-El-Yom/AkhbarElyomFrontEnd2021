@model List<MVC.Data.News>;
@{
}



<!--Sidebar Page Container-->
<div class="sidebar-page-container">
    <div class="auto-container">
        <div class="row clearfix">





            <div class="column col-md-8 col-sm-8 col-xs-12" style="float:right;">
                <!--Sec Title-->
                <div class="sec-title" style="direction: rtl;">
                    <h2>@Model[0].Section.SecTitle</h2>
                </div>
                <partial name="Sliders/_OwlSingleItemCarousel" model="@Model.OrderByDescending(e => e.AddedDate).Take(5).ToList()" />

                <div class="row clearfix">

                    <div class="column col-md-6 col-sm-6 col-xs-12">
                        @foreach (var item in Model.OrderByDescending(e => e.AddedDate).Skip(5).ToList())
                        {
                            <!--News Block Two-->
                            <div class="news-block-two with-margin">
                                <div class="inner-box">
                                    <div class="image">
                                        <a href="blog-single-2.html"><img class="wow fadeIn" data-wow-delay="0ms" data-wow-duration="2500ms" src="https://images.akhbarelyom.com/images/images/small/@item.Picture1.PictureName" alt="" /></a>
                                        
                                    </div>
                                    <div class="lower-box">
                                        <h3><a href="#">@item.Title</a></h3>
                                    </div>
                                </div>
                            </div>
                        }


                    </div>
                </div>


                <!-- Styled Pagination -->
                <div class="styled-pagination" style="float:right">
                    <ul class="clearfix">
                        <li><a href="#" class="active">1</a></li>
                        <li><a href="#">2</a></li>
                        <li><a href="#">3</a></li>
                        <li><a class="next" href="#"><span class="fa fa-angle-left"></span></a></li>
                    </ul>
                </div>


            </div>




            <!-- #include file ="sidebar.aspx" -->
            <!--Sidebar Side-->



        </div>

    </div>
</div>
<!--End Sidebar Page Container-->
